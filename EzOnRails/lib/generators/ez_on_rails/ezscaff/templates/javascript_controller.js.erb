import { Controller } from "@hotwired/stimulus"

/**
 * Stimulus controller for <%= class_name %> actions.
 */
export default class extends Controller {
    static targets = []

    /**
     * Constructor.
     */
    connect() { }

    <%- attributes.each_with_index do |attr, i| -%>
    /**
     * Called if the <%= attr.name %> in the <%= class_name %> form was changed.
     */
    onChange<%= attr.name.camelize(:upper) %>() { }

    <%- end -%>
}
